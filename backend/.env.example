# Copy to .env and adjust as needed.
# The backend reads env vars directly (no prefix) via pydantic-settings.

# dev|staging|prod
ENV=dev

# Comma-separated origins for CORS (leave blank to disable)
# Example: CORS_ORIGINS=http://localhost:3000,http://localhost:5173
CORS_ORIGINS=

# Database connection string
# Default is local sqlite file. For Postgres/Supabase, set a postgres:// URL.
DATABASE_URL=sqlite:///./app.db

# Optional: set during deploy so /api/version reports the build SHA
GIT_SHA=
